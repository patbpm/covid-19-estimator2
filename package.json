{
  "name": "covid-19-estimator-js",
  "version": "1.0.0",
  "repository": "git@github.com:BuildforSDG-Cohort1-Assessment/covid-19-estimator-js.git",
  "license": "MIT",
  "main": "server.js",
  "directories": {
    "lib": "lib"
  },
  "private": false,
  "scripts": {
    "test": "jest",
    "start": "node server.js"
  },
  "devDependencies": {
    "jest": "^25.2.7",
    "jest-pytest": "^0.7.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "py"
    ],
    "runner": "<rootDir>/pytest-runner",
    "testPathIgnorePatterns": [
      "<rootDir>/audits/test_utils.py"
    ],
    "testMatch": [
      "<rootDir>/audits/**/test_*.py"
    ]
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "on-covid-19": "^0.1.3"
  },
  "browserslist": [
    "last 1 Chrome versions"
  ]
}
